{
	"comments": {
		"lineComment": "*>"
	},
	"brackets": [
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"\"",
			"\""
		],
		[
			"'",
			"'"
		],
		[
			"if",
			"end-if"
		],
		[
			"call",
			"end-call"
		],
		[
			"evaluate",
			"end-evaluate"
		],
		[
			"perform",
			"end-perform"
		],
		[
			"string",
			"end-string"
		],
		[
			"unstring",
			"end-unstring"
		],
		[
			"compute",
			"end-compute"
		],
		[
			"$if",
			"$end"
		]
	],
	"autoClosingPairs": [
		{
			"open": "[",
			"close": "]",
			"notIn": [
				"string",
				"comment"
			]
		},
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "'",
			"close": "'",
			"notIn": [
				"string",
				"comment"
			]
		},
		{
			"open": "\"",
			"close": "\"",
			"notIn": [
				"string",
				"comment"
			]
		}
	],
	"surroundingPairs": [
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"\"",
			"\""
		],
		[
			"'",
			"'"
		]
	],
	"folding": {
		"markers": {
			"start": "^\\s*\\$region\\b",
			"end": "^\\s*\\$end-region\\b"
		}
	},
	"wordPattern": "([#a-zA-Z0-9_-]+|[a-zA-Z0-9_-]+)"
}